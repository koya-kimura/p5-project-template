    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-glsl": "^1.5.5"
